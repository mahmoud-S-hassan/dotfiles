  //   "interval": 5,
  //   "format": " GPU {} 󰈐 ",
  //   "exec": "cat /sys/class/hwmon/hwmon6/fan2_input",
  //   "tooltip": true,
  //   "tooltip-format": "GPU Fan: {} RPM ",
  // },
  // ----------------------------------------------------------------------

  "custom/cliphist": {
    "format": " ",
    "on-click": "sleep 0.1 && ~/dotfiles/hypr_custom/shellScript/cliphist.sh",
    "on-click-right": "sleep 0.1 && ~/dotfiles/hypr_custom/shellScript/cliphist.sh d",
    "on-click-middle": "sleep 0.1 && ~/dotfiles/hypr_custom/shellScript/cliphist.sh w",
    "tooltip-format": "Left: Open clipboard Manager\nRight: Delete an entry\nMiddle: Clear list",
  },

  // Power Profiles Daemon
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      // "default": "",
      "performance": "  ",
      "balanced": "  ",
      "power-saver": "  ",
    },
  },
}
