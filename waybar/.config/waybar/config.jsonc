  //   "interval": 5,
  //   "format": " CPU {} 󰈐 ",
  //   "exec": "cat /sys/class/hwmon/hwmon6/fan1_input",
  //   "tooltip": true,
  //   "tooltip-format": "CPU Fan: {} RPM ",
  // },
